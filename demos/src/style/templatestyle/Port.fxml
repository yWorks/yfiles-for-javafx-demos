<?import com.yworks.yfiles.graph.styles.PortTemplate?>
<?import javafx.scene.shape.Polygon?>
<?import java.net.URL?>

<!-- A template for a simple port style showing a hexagon that is styled using "TemplateStyle.css. -->
<PortTemplate fx:id="portNode"
              xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <stylesheets><URL value="@TemplateStyle.css"/></stylesheets>
  <Polygon points="0, -5, 5, -2.5, 5, 2.5, 0, 5, -5, 2.5, -5, -2.5, 0, -5"  styleClass="port"/>
</PortTemplate>
